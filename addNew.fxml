<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #D76CF7;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddNew">
    <children>
        <TextField fx:id="txtUsername" layoutX="202.0" layoutY="107.0" prefHeight="33.0" prefWidth="191.0" promptText="New UserName" style="-fx-background-color: #EAC7F4;" />
        <PasswordField fx:id="txtPassword" layoutX="202.0" layoutY="180.0" prefHeight="33.0" prefWidth="191.0" promptText="Password" style="-fx-background-color: #EAC7F4;" />
        <PasswordField fx:id="txtPassword2" layoutX="202.0" layoutY="249.0" prefHeight="33.0" prefWidth="191.0" promptText="ReEnter Password" style="-fx-background-color: #EAC7F4;" />
        <Button fx:id="saveButton" layoutX="264.0" layoutY="318.0" mnemonicParsing="false" onAction="#SaveButton" prefHeight="33.0" prefWidth="82.0" style="-fx-background-color: #CDF219;" text="SAVE" wrapText="true">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font></Button>
      <Label layoutX="155.0" layoutY="33.0" prefHeight="33.0" prefWidth="383.0" text="ENTER NEW USERNAME &amp; PASSWORD" textFill="#0626f8">
         <font>
            <Font name="Courier New Bold" size="18.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
